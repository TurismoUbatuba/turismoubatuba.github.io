                        function empty(str){
                            if (typeof str == 'undefined' || !str || str.length === 0 || str === "" || !/[^\s]/.test(str) || /^\s*$/.test(str) || str.replace(/\s/g,"") === ""){
                               return true;
                            }else{
                               return false;
                            }
                        }
                    
                        function marco(){
                            var email = document.getElementById("email").value;
                            if (empty(email)) {
                                alert('E-Mail nao preenchido!');
